{
    "name": "codyjheiser/laravel-db2-eloquent",
    "description": "Laravel Eloquent extensions for IBM DB2 databases with column mapping, multi-column relationships, and auto-filtering",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cody J Heiser",
            "email": "cody@example.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rufhausen/db2-driver"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0",
        "nesbot/carbon": "^2.0|^3.0",
        "rufhausen/db2-driver": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "CodyJHeiser\\Db2Eloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodyJHeiser\\Db2Eloquent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodyJHeiser\\Db2Eloquent\\Db2EloquentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testsuite Unit",
        "test-integration": "vendor/bin/phpunit --testsuite Integration",
        "test-all": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
